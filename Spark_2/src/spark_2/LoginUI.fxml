      <Label layoutX="57.0" layoutY="162.0" text="Password:" />
      <TextField fx:id="username" layoutX="140.0" layoutY="88.0" />
      <PasswordField fx:id="password" layoutX="140.0" layoutY="157.0" />
      <Label layoutX="153.0" layoutY="37.0" text="Welcome to Spark">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="loginLabel" layoutX="125.0" layoutY="240.0" prefHeight="21.0" prefWidth="194.0" />
      <Button fx:id="cancel" layoutX="39.0" layoutY="235.0" mnemonicParsing="false" onAction="#cancelLogin" text="Cancel" />
    </children>

</AnchorPane>
